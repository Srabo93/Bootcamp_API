{
  "name": "docker_node_mongo_starter",
  "version": "1.0.0",
  "description": "docker starter with node js and mongodb services",
  "scripts": {
    "start": "node ../dist/server.js",
    "build": "npx tsc",
    "format": "prettier --write {**/,}*.js",
    "lint": "prettier --check {**/,}*.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -L ./server.ts\""
  },
  "author": "Armin Srabovic",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "bottleneck": "^2.19.5",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.15.0",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^3.0.7",
    "mongoose": "^6.0.9",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-geocoder": "^4.2.0",
    "nodemailer": "^6.9.1",
    "simple-node-logger": "^18.12.23",
    "slugify": "^1.6.5",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.5",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}
