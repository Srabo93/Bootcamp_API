{
  "name": "docker_node_mongo_starter",
  "version": "1.0.0",
  "description": "docker starter with node js and mongodb services",
  "scripts": {
    "start": "node ../dist/server.js",
    "build": "npx tsc",
    "format": "prettier --write {**/,}*.js",
    "lint": "prettier --check {**/,}*.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon  ./server.ts\""
  },
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongodb": "^3.0.7",
    "mongoose": "^6.0.9",
    "simple-node-logger": "^18.12.23"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.5",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}
